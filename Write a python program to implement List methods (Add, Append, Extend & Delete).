my_list = [1, 2, 3]
new_list = my_list + [4]
print("List after adding element using +:", new_list)
my_list.append(4)
print("List after adding element using append():", my_list)
my_list.extend([5, 6])
print("List after extending using extend():", my_list)
del my_list[0]
print("List after deleting using del:", my_list)
my_list.remove(3)
print("List after deleting using remove():", my_list)
