nested_list = [[1, 2, 3], [4, 5, 6], [7, 8, 9]]
print("Length of the list:", len(nested_list))
list1 = [1, 2, 3]
list2 = [4, 5, 6]
concatenated_list = list1 + list2
print("Concatenated list:", concatenated_list)
if 4 in concatenated_list:
    print("4 is present in the list")
else:
    print("4 is not present in the list")
for element in concatenated_list:
    print(element)
print("First element of the concatenated list:", concatenated_list[0])
print("Sliced list:", concatenated_list[2:5])
